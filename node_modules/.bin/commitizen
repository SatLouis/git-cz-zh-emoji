#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/朝闻道/AppData/Local/pnpm/global/5/.pnpm/commitizen@4.3.0_@types+node@20.11.30_typescript@5.4.3/node_modules/commitizen/bin/node_modules:/mnt/c/Users/朝闻道/AppData/Local/pnpm/global/5/.pnpm/commitizen@4.3.0_@types+node@20.11.30_typescript@5.4.3/node_modules/commitizen/node_modules:/mnt/c/Users/朝闻道/AppData/Local/pnpm/global/5/.pnpm/commitizen@4.3.0_@types+node@20.11.30_typescript@5.4.3/node_modules:/mnt/c/Users/朝闻道/AppData/Local/pnpm/global/5/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/朝闻道/AppData/Local/pnpm/global/5/.pnpm/commitizen@4.3.0_@types+node@20.11.30_typescript@5.4.3/node_modules/commitizen/bin/node_modules:/mnt/c/Users/朝闻道/AppData/Local/pnpm/global/5/.pnpm/commitizen@4.3.0_@types+node@20.11.30_typescript@5.4.3/node_modules/commitizen/node_modules:/mnt/c/Users/朝闻道/AppData/Local/pnpm/global/5/.pnpm/commitizen@4.3.0_@types+node@20.11.30_typescript@5.4.3/node_modules:/mnt/c/Users/朝闻道/AppData/Local/pnpm/global/5/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../commitizen@4.3.0_@types+node@20.11.30_typescript@5.4.3/node_modules/commitizen/bin/commitizen" "$@"
else
  exec node  "$basedir/../../../../../commitizen@4.3.0_@types+node@20.11.30_typescript@5.4.3/node_modules/commitizen/bin/commitizen" "$@"
fi
